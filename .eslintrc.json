 { 
  "extends": ["next/core-web-vitals",
   "next/typescript",
   "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": ["import"],
  "rules": {
    "import/no-cycle": ["error", { "maxDepth": 1 }],
     "no-console": "off",
     "no-debugger": "warn"
  }
}
